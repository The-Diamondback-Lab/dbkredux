{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch With Local Backend",
      "program": "${workspaceFolder}/server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch With Production Backend",
      "program": "${workspaceFolder}/server.js",
      "env": {
        "NODE_ENV": "development",
        "PRODUCTION_BACKEND": "true"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ],
  "compounds": []
}
